    "@sveltejs/kit": "^2.13.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "autoprefixer": "^10.4.20",
    "bits-ui": "^1.0.0-next.74",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^10.0.1",
